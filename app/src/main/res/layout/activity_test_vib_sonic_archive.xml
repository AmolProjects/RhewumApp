<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.VibSonicArchiveActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintDimensionRatio="1:0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context=".Activity.ArchiveActivity">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="50dp"
            app:layout_constraintTop_toTopOf="parent" />
        <!-- AppBar layout for using Toolbar as AppBar -->
        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/appBarLayout"
            android:layout_width="0dp"
            android:layout_height="@dimen/header_layout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5">

                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/my_toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/header_layout"
                    android:background="@color/gray"
                    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                    app:layout_constraintDimensionRatio="w,1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

                <!-- Title, back button, and icons are inside the AppBar -->
                <TextView
                    android:id="@+id/txtTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/handle_go"
                    android:gravity="center"
                    android:text="@string/archive"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    app:layout_anchor="@id/my_toolbar"
                    app:layout_anchorGravity="center"
                    app:layout_constraintBottom_toBottomOf="@+id/txtBack"
                    app:layout_constraintEnd_toEndOf="@+id/my_toolbar"
                    app:layout_constraintStart_toStartOf="@+id/my_toolbar"
                    app:layout_constraintTop_toTopOf="@+id/txtBack" />

                <TextView
                    android:id="@+id/txtBack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:fontFamily="@font/handle_go"
                    android:text="@string/back"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="15sp"
                    app:layout_anchor="@id/my_toolbar"
                    app:layout_anchorGravity="start"
                    app:layout_constraintBottom_toBottomOf="@+id/images"
                    app:layout_constraintStart_toEndOf="@+id/images"
                    app:layout_constraintTop_toTopOf="@+id/images" />

                <ImageView
                    android:id="@+id/images"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:layout_anchor="@id/my_toolbar"
                    app:layout_anchorGravity="start"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/my_toolbar"
                    app:srcCompat="@drawable/baseline_arrow_back_24" />

                <TextView
                    android:id="@+id/activity_mesh_info_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:fontFamily="@font/handle_go"
                    android:text="@string/more"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="15sp"
                    app:layout_anchor="@id/my_toolbar"
                    app:layout_anchorGravity="end"
                    app:layout_constraintBottom_toBottomOf="@+id/image_vib"
                    app:layout_constraintEnd_toStartOf="@+id/image_vib"
                    app:layout_constraintTop_toTopOf="@+id/image_vib" />

                <ImageView
                    android:id="@+id/image_vib"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    app:layout_anchor="@id/my_toolbar"
                    app:layout_anchorGravity="end"
                    app:layout_constraintBottom_toBottomOf="@+id/txtTitle"
                    app:layout_constraintEnd_toEndOf="@+id/my_toolbar"
                    app:layout_constraintTop_toTopOf="@+id/txtTitle"
                    app:srcCompat="@drawable/baseline_arrow_forward_24" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.appbar.AppBarLayout>

        <ScrollView
            android:id="@+id/scoll"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/appBarLayout"
            app:layout_constraintVertical_bias="0.5">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1:0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/scoll">

                <TextView
                    android:id="@+id/activity_vib_sonic_dba_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="2dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/activity_vib_sonic_archieve_soundGraph"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.5" />

                <com.github.mikephil.charting.charts.BarChart
                    android:id="@+id/activity_vib_sonic_archieve_soundGraph"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="h,1:1.1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/activity_vib_sonic_dba_tv" />

                <TextView
                    android:id="@+id/txt_DateTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="32dp"
                    android:text="Time"
                    android:textColor="@color/black0"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/activity_vib_sonic_archieve_soundGraph" />

                <TextView
                    android:id="@+id/txt_MeasureMentTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp"
                    android:text="@string/measurement_times"
                    android:textColor="@color/black0"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_DateTime" />

                <TextView
                    android:id="@+id/txt_MeasureMentTimeVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:text="@string/measurement_times"
                    android:textColor="@color/black0"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintStart_toEndOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toTopOf="@+id/txt_MeasureMentTime" />

                <TextView
                    android:id="@+id/txt_Frequency"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:text="@string/frequency"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_MeasureMentTime" />

                <TextView
                    android:id="@+id/txt_SoundPressure"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="45dp"
                    android:text="@string/sound_pressure_level"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_Frequency"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/txt_Frequency" />

                <TextView
                    android:id="@+id/txt_32Hz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_32_hz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_Frequency" />

                <TextView
                    android:id="@+id/txt_32HzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_32Hz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_Frequency" />

                <TextView
                    android:id="@+id/txt_63Hz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_63_hz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_32HzVal" />

                <TextView
                    android:id="@+id/txt_63HzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_32Hz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_63Hz" />

                <TextView
                    android:id="@+id/txt_125Hz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_125_hz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_63HzVal" />

                <TextView
                    android:id="@+id/txt_125HzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_125Hz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_63Hz" />

                <TextView
                    android:id="@+id/txt_250Hz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_250_hz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_125HzVal" />

                <TextView
                    android:id="@+id/txt_250HzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_250Hz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_125Hz" />

                <TextView
                    android:id="@+id/txt_500Hz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_500_hz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_250Hz" />

                <TextView
                    android:id="@+id/txt_500HzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_250Hz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_500Hz" />

                <TextView
                    android:id="@+id/txt_1kHz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_1_khz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_500Hz" />

                <TextView
                    android:id="@+id/txt_1kHzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_1kHz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_500Hz" />

                <TextView
                    android:id="@+id/txt_2kHz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_2_khz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_1kHz" />

                <TextView
                    android:id="@+id/txt_2kHzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_1kHz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_2kHz" />

                <TextView
                    android:id="@+id/txt_4kHz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_4_khz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_2kHz" />

                <TextView
                    android:id="@+id/txt_4kHzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_4kHz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toTopOf="@+id/txt_4kHz" />

                <TextView
                    android:id="@+id/txt_8kHz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_8_khz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_4kHz" />

                <TextView
                    android:id="@+id/txt_8kHzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_8kHz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_4kHz" />

                <TextView
                    android:id="@+id/txt_16kHz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/_16_khz"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_8kHz" />

                <TextView
                    android:id="@+id/txt_16kHzVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/header_backgrounds"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_16kHz"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toBottomOf="@+id/txt_8kHz" />

                <TextView
                    android:id="@+id/txt_Mean"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:text="@string/mean_level"
                    android:textColor="@color/black0"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_16kHz" />

                <TextView
                    android:id="@+id/txt_MeanVal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_7_18_db_a"
                    android:textColor="@color/black0"
                    android:textSize="@dimen/text_size_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_Mean"
                    app:layout_constraintStart_toStartOf="@+id/txt_SoundPressure"
                    app:layout_constraintTop_toTopOf="@+id/txt_Mean" />

                <TextView
                    android:id="@+id/txt_info"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="16dp"
                    android:text="@string/text_info"
                    android:textSize="@dimen/text_size_15"
                    app:layout_constraintDimensionRatio="w,1:0"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/txt_MeasureMentTime"
                    app:layout_constraintTop_toBottomOf="@+id/txt_MeanVal" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/bt_save"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/layout_margin_45"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="@dimen/layout_margin_3"
                    android:layout_marginBottom="4dp"
                    android:background="@color/header_backgrounds"
                    android:text="@string/save"
                    android:textColor="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintDimensionRatio="w,1:0"
                    app:layout_constraintEnd_toStartOf="@+id/bt_share"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="@+id/txt_info"
                    app:layout_constraintTop_toBottomOf="@+id/txt_info" />

                <Button
                    android:id="@+id/bt_share"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/layout_margin_45"
                    android:layout_marginStart="@dimen/layout_margin_3"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="4dp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_comm"
                    android:text="@string/share"
                    android:textColor="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintDimensionRatio="w,1:0"
                    app:layout_constraintEnd_toEndOf="@+id/txt_info"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/bt_save"
                    app:layout_constraintTop_toBottomOf="@+id/txt_info" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/layout_margin_30"
                    android:src="@drawable/baseline_turn_right_24"
                    app:layout_constraintBottom_toBottomOf="@+id/bt_share"
                    app:layout_constraintEnd_toEndOf="@+id/bt_share"
                    app:layout_constraintTop_toTopOf="@+id/bt_share" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

